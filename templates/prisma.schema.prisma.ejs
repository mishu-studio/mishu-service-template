generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "<%= database === 'MongoDB' ? 'mongodb' : 'postgresql' %>"
  url      = env("DATABASE_URL")
}

model User {
  id           Int       @id @default(autoincrement())
  name         String
  passwordHash String
  email        String    @unique
}
